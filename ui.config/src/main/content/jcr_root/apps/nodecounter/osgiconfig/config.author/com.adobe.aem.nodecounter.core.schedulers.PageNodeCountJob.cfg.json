{
  "enabled": "$[env:NODE_COUNTER_ENABLED;default=true]",
  "rootPath": "$[env:NODE_COUNTER_ROOT_PATH;default=/content]",
  "scheduler.expression": "$[env:NODE_COUNTER_CRON;default=0 0 0 * * ?]",
  "highThreshold": "$[env:NODE_COUNTER_HIGH_THRESHOLD;default=2048]",
  "mediumThreshold": "$[env:NODE_COUNTER_MEDIUM_THRESHOLD;default=1024]"
}

